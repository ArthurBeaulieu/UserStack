<h2>Profile</h2>
<div id="profile-page">
  <p id="error-output"></p>
  <img src="{{avatar}}" alt="user-avatar">
  <p>{{username}}</p>
  <p>{{email}}</p>
  <p>{{code}}</p>
  <p>{{depth}}</p>
  <p>{{godfather}}</p>
  <p>{{registration}}</p>
  <p>{{lastLogin}}</p>
  <ul class="roles">
    {{#each roles}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <a href="/profile/edit">Edit profile</a>
  <a href="/home">Homepage</a>
</div>
